<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Estado pressionado -->
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="12dp" />
            <solid android:color="#A3B3C5" />
        </shape>
    </item>
    <!-- Estado normal -->
    <item>
        <shape>
            <corners android:radius="12dp" />
            <solid android:color="#F0F6FD" />
        </shape>
    </item>
</selector>